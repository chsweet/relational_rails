<h1>Movies</h1>
  <% @movies.each do |movie| %>
    <h3><%= movie.title %></h3>
    <p>Has Oscar: <%= movie.oscar %></p>
    <p>Nominations: <%= movie.nominations %></p>
    <%= link_to "Edit #{movie.title}" , "/movies/#{movie.id}/edit" %>
    <br>
    <br>
    <%= link_to "Delete #{movie.title}" , "/movies/#{movie.id}", method: :delete %>
<% end %>
