<%= form_with url: "/movies/#{@movie.id}", method: :patch, local: true do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title, :value => @movie.title %>
  <%= form.label :oscar %>
  <%= form.check_box :oscar %>
  <%= form.label :nominations %>
  <%= form.number_field :nominations, :value => @movie.nominations %>
  <%= form.submit "Update Movie" %>
<% end %>
