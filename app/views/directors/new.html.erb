<%= form_with url: "/directors", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :oscar %>
  <%= form.check_box :oscar %>
  <%= form.label :nominations %>
  <%= form.number_field :nominations %>
  <%= form.submit 'Create Director' %>
<% end %>
